{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  // "content_scripts": [
  //   {
  //     "matches": ["<all_urls>"],
  //     "js": ["content.js"]
  //   }
  // ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "save-this-link": {
      "suggested_key": {
        "windows": "Ctrl+Shift+S",
        "mac": "Command+Ctrl+S",
        "linux": "Ctrl+Shift+S"
      },
      "description": "Save this link"
    }
  },
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs"]
}
